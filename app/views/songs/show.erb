<!DOCTYPE html>
<html>

<head>
  <title></title>
</head>
<body>
  <% if flash.has?(:message) %>
    <%= flash[:message] %>
  <% end %>

<h3> <%= @song.name %> </h3>
 <p><a href="/artists/<%= @song.artist.slug %>"> <%= @song.artist.name %> </a></p>
<ul>  <% @song.genres.each do |genre| %>
<li>  <a href="/genres/<%= genre.slug %>"> <%= genre.name %> </a> </li>
<% end %> </ul>

</body>
</html>
